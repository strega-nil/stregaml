type V2 {
  data = record {
    x0: Int32;
    x1: Int32;
  };
}

func map(p: V2, f: func(Int32) -> Int32) -> V2 {
  V2::{ x0 = f(p.x0); x1 = f(p.x1) }
}

func double(i: Int32) -> Int32 {
  MUL(i, 2)
}

func main() -> V2 {
  let v: V2 = V2::{ x0 = 2014; x1 = 9 };
  map(v, double)
}

