(jbuild_version 1)

(library (
  (name cafec)
  (modules (cafec))
  (libraries (
    base
    cafec_containers
    cafec_interpreter
    cafec_llvm
    cafec_parse
    cafec_typed_ast))
  (flags (:standard -safe-string -w +a-4 -open Base -open Cafec_containers))))

(executables (
  (names (main))
  (modules (main))
  (libraries (cafec))
  (flags (:standard -safe-string -w +a-4 -open Base))))

(install (
  (section bin)
  (package cafec)
  (files ((main.exe as cafec)))))
