(jbuild_version 1)

(library (
  (name cafec)
  (modules (cafec))
  (libraries (
    base cafec_parse cafec_containers cafec_typed_ast cafec_interpreter))
  (flags (:standard -safe-string -w +a-4 -open Base))))

(executables (
  (names (main))
  (modules (main))
  (libraries (cafec))
  (flags (:standard -safe-string -w +a-4 -open Base))))

(install (
  (section bin)
  (package cafec)
  (files ((main.exe as cafec)))))
